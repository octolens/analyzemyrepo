model github_raw_users {
  id               Int       @id @default(autoincrement())
  login            String?
  avatar_url       String?
  gravatar_id      String?
  type             String?
  site_admin       Boolean?
  name             String?
  company          String?
  blog             String?
  location         String?
  email            String?
  hireable         String?
  bio              String?
